		<UseWindowsForms>true</UseWindowsForms>
		<UseWPF>true</UseWPF>
		<DpiAwareness>PerMonitorV2</DpiAwareness>
		<ApplicationIcon>CWRPFExplorer.ico</ApplicationIcon>
		<Copyright>dexyfex</Copyright>
